function collect(reason)
    if (reason) then
		printf("collectgarbage reason: %s",reason)
		reason = nil
	end
    printf("collectgarbage before=%sKb",collectgarbage("count")*1024)
    collectgarbage("collect")
	collectgarbage("collect")
    printf("collectgarbage after=%sKb",collectgarbage("count")*1024)
end
